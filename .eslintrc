{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": ["standard", "plugin:prettier/recommend"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "prettier/prettier": "warn"
    }
}
